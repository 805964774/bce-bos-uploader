    "pack:win32": "electron-packager . $npm_package_productName --overwrite --asar --out=dist/win --platform=win32 --arch=ia32 --version-string.CompanyName=\"Baidu Inc.\" --version-string.ProductName=\"BaiduCloudEngine BOS Uploader\" --version-string.ProductVersion=0.0.1 --icon=resources/icon.ico",
